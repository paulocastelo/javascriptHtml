{
  "name": "Osmosis",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "activeTab",
    "browserAction"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Osmosis",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  }
  
}
